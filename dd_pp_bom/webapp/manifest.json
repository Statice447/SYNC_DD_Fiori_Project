{
  "_version": "1.48.0",
  "sap.app": {
    "id": "fiorippbom",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.11.2",
      "toolsId": "45d1c345-dd02-4bd4-895e-9c47032fbb56"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZGWC702_PP_BOM_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.108.18",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "fiorippbom.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "fcl": {
        "type": "sap.ui.model.json.JSONModel"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "fiorippbom.view",
        "controlId": "fcl",
        "transition": "slide"
      },
      "routes": [
        {
          "name": "Main",
          "pattern": ":layout:",
          "target": [
            "mainView"
          ]
        },
        {
          "name": "Detail",
          "pattern": "detail/{layout}/:BOMid:/:GDNAME:/:DELFLAG:",
          "target": ["mainView", "detailView"]
        }
      ],
      "targets": {
        "mainView": {
          "controlAggregation": "beginColumnPages",
          "clearControlAggregation": true,
          "viewId": "Main",
          "viewName": "Main"
        },
        "detailView": {
          "viewName": "Detail",
          "controlAggregation": "midColumnPages",
          "clearControlAggregation": true
        }
      }
    },
    "rootView": {
      "viewName": "fiorippbom.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    },
    "config": {
      "fullWidth": true
    }
  }
}
